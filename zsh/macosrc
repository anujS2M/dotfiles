# Mac OSX specific application alias.
alias studio='open -a /Applications/Android\ Studio.app'
alias rmine='open -a /Applications/RubyMine.app'
alias goland='open -a /Applications/GoLand.app'

function portscan()
{
  lsof -i :$1
}
alias portscan='portscan'

alias rsp='run_rspec'
function run_rspec()
{
  rspec ./$1
}